% General.
doubleCompTol = 1.0e-14

% Dynamic programming.
nStages = 2
nStatesDim = 2
nControlsDim = 1
nDisturbanceDim = 1
% Number of parameters should be consistent with what the state describes.
nInferenceParamsDim = 1
initialState = 7.0  9.0
controlsLowerBounds = 1.0
controlsUpperBounds = 10.0
noiseModel = 1
forwardModel = 1
nStochOptPerState = 100
simulateTrajectories = 1
nTrajectories = 10
%nTrajectories = 5000

% ADP value function approximation.
featuresChoice = 2
pOrder = 4
coefsConstructionMethod = 1
nRegressionSamples = 100
%nRegressionSamples = 10000

% Stochastic Optimization.
optMethod = 6
maxOptIters = 50
relXNormTerminateTol = 1.0e-6
nConsecRelXNormTerminateTol = 5
nObjMC = 10
%nObjMC = 1000
userXInitial = 5.5
randomizeXInitial = 1
nFinalObjHighQualityMC = 1000
%nFinalObjHighQualityMC = 100000
displayOptProgress = 0
displayOptSummary = 0

%% SARM.
%checkInitialGradient = 0
%% SARMGainSeq = 1 is a/k, and SARMGainSeq = 2 is SPSA gain sequence.
%SARMGainSeq = 1
%% Only applicable to SARMGainSeq = 1.
%gainMultiplier = 5
%% Only applicable to SARMGainSeq = 2.
%detectSPSAParams = 1
%SPSAInputa = 0.16
%SPSAInputc = 0.16
%SPSAInputA = 100.0
%SPSAInputalpha = 0.602
%SPSAInputgamma = 0.101
